{
  // GCP-specific configuration overrides for Production
  "Messaging": {
    "Provider": "RabbitMQ",
    "Host": "rabbitmq.gcp.prod.internal",
    "Port": 5672,
    "VirtualHost": "/production",
    "Username": "${RABBITMQ_USERNAME}",
    "Password": "${RABBITMQ_PASSWORD}",
    "Outbox": {
      "Enabled": true,
      "QueryDelay": 50,
      "QueryLimit": 200
    }
  },

  "Consul": {
    "Host": "http://consul.gcp.prod.internal:8500"
  },

  "ReverseProxy": {
    "Clusters": {
      "user-service": {
        "Destinations": {
          "primary": {
            "Address": "http://user-service.gcp.internal"
          }
        }
      },
      "catalog-service": {
        "Destinations": {
          "primary": {
            "Address": "http://catalog-service.gcp.internal"
          }
        }
      },
      "order-service": {
        "Destinations": {
          "primary": {
            "Address": "http://order-service.gcp.internal"
          }
        }
      },
      "notification-service": {
        "Destinations": {
          "primary": {
            "Address": "http://notification-service.gcp.internal"
          }
        }
      }
    }
  },

  "Jaeger": {
    "AgentHost": "jaeger.gcp.internal",
    "AgentPort": 6831
  },

  "Seq": {
    "ServerUrl": "http://seq.gcp.internal:5341"
  }
}
