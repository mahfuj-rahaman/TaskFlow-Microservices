{
  // AWS-specific configuration overrides for Production
  "Messaging": {
    "Provider": "AmazonSQS",
    "AwsRegion": "us-east-1",
    "AwsAccessKey": "${AWS_ACCESS_KEY}",
    "AwsSecretKey": "${AWS_SECRET_KEY}",
    "Outbox": {
      "Enabled": true,
      "QueryDelay": 50,
      "QueryLimit": 200
    }
  },

  "Consul": {
    "Host": "http://consul.aws.prod.internal:8500"
  },

  "ReverseProxy": {
    "Clusters": {
      "user-service": {
        "Destinations": {
          "primary": {
            "Address": "http://user-service.aws.internal"
          }
        }
      },
      "catalog-service": {
        "Destinations": {
          "primary": {
            "Address": "http://catalog-service.aws.internal"
          }
        }
      },
      "order-service": {
        "Destinations": {
          "primary": {
            "Address": "http://order-service.aws.internal"
          }
        }
      },
      "notification-service": {
        "Destinations": {
          "primary": {
            "Address": "http://notification-service.aws.internal"
          }
        }
      }
    }
  },

  "Jaeger": {
    "AgentHost": "jaeger.aws.internal",
    "AgentPort": 6831
  },

  "Seq": {
    "ServerUrl": "http://seq.aws.internal:5341"
  }
}
