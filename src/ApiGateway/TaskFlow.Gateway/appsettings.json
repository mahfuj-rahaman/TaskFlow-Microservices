{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "Host": "http://consul:8500"
  },
  "ReverseProxy": {
    "Clusters": {
      "task-service": {
        "Destinations": {
          "destination1": {
            "Address": "http://task-service"
          }
        }
      }
    },
    "Routes": {
      "task-route": {
        "ClusterId": "task-service",
        "Match": {
          "Path": "/api/v1/tasks/{**catch-all}"
        }
      }
    }
  }
}
