{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Error"
    }
  },
  "ConnectionStrings": {
    "TaskDb": ""
  },
  "DatabaseProvider": "PostgreSQL",
  "EnableSensitiveDataLogging": false,
  "Redis": {
    "ConnectionString": "",
    "InstanceName": "TaskFlow:Prod:",
    "ConnectTimeout": 5000,
    "SyncTimeout": 5000,
    "AbortOnConnectFail": false
  },
  "RabbitMQ": {
    "Host": "",
    "Port": 5672,
    "Username": "",
    "Password": "",
    "VirtualHost": "/",
    "QueueName": "taskflow-tasks-prod",
    "RequestedHeartbeat": 60,
    "AutomaticRecoveryEnabled": true,
    "NetworkRecoveryInterval": 10
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": []
  },
  "Swagger": {
    "Enabled": false
  },
  "HealthChecks": {
    "Enabled": true,
    "Path": "/health"
  }
}
